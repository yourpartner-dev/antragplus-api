{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node@3",
      "config": {
        "runtime": "nodejs22.16.0"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/start.js"
    }
  ],
  "buildCommand": "pnpm install --include=dev && pnpm run build",
  "installCommand": "pnpm install --include=dev",
  "outputDirectory": "dist"
}