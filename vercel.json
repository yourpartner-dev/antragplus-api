{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node@3",
      "config": {
        "runtime": "nodejs22.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/start.js"
    }
  ],
  "buildCommand": "pnpm install && pnpm run build",
  "installCommand": "pnpm install"
}