{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs22.x",
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "./build.sh",
  "outputDirectory": "./dist",
  "framework": null
}